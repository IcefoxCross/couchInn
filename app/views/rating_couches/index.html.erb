<p id="notice"><%= notice %></p>

<h1>Listing Rating Couches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rating_couches.each do |rating_couch| %>
      <tr>
        <td><%= link_to 'Show', rating_couch %></td>
        <td><%= link_to 'Edit', edit_rating_couch_path(rating_couch) %></td>
        <td><%= link_to 'Destroy', rating_couch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rating couch', new_rating_couch_path %>
