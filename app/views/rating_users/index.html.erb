<p id="notice"><%= notice %></p>

<h1>Listing Rating Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rating_users.each do |rating_user| %>
      <tr>
        <td><%= link_to 'Show', rating_user %></td>
        <td><%= link_to 'Edit', edit_rating_user_path(rating_user) %></td>
        <td><%= link_to 'Destroy', rating_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rating user', new_rating_user_path %>
